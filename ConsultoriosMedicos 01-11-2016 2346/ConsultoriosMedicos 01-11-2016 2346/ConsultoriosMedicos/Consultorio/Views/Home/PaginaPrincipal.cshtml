@{
    ViewBag.Title = "Bienvenido al sistema de consultorios medicos";
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
           
        </div>
    </section>
}
@using (Html.BeginForm()) 
{ 
 <div>@Html.Label("Usuario")</div>
 <div>@Html.TextBox("nombre")</div>
  <div>@Html.Label("Password")</div>
 <div>@Html.TextBox("password")</div>
 <input type="submit" value="LogIn" />
}

